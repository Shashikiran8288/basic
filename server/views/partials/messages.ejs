<div>
  <% if(typeof errors!= 'undefined') { %>
<%    errors.forEach(function(error){ %>
<p id="flash"> <%= error.msg %></p>
<%    })        %>
<% } %>

<% if(success_msg!= '') { %>
<p id="flash" ><%=success_msg %> </p>
<% } %>

<% if(error_msg != '') { %>
<p id="flash"><%= error_msg %></p> 
<% } %> 
<% if(error != '') { %>
<p id="flash"><%= error %> </p>
<% } %> 
</div>

